package test.server;

import test.common.Counter;

public class CounterImpl implements Counter {

    private static final long serialVersionUID = -1881637933978100698L;
    int myNumber;

    CounterImpl(int myNumber) {
        this.myNumber = myNumber;
    }

    @Override
    public void test() {
        System.out.println("CounterImpl::test() .. myNumber=" + myNumber);
    }

    @Override
    public int getNumber() {
        return myNumber;
    }

}
